    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@opensource-observer/utils": "workspace:*",
    "@types/lodash": "^4.17.6",
    "lodash": "^4.17.21",
