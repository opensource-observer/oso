    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@jest/globals": "^30.0.2",
    "@types/lodash": "^4.17.6",
    "ethers": "^6.14.4",
    "lodash": "^4.17.21"
  }
