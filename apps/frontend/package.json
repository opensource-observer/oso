    "qs": "^6.14.0",
    "random-words": "^2.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-instantsearch": "^7.15.4",
    "react-instantsearch-nextjs": "^0.4.5",
    "react-markdown": "^10.1.0",
    "react-use": "^17.6.0",
    "serve": "^14.2.4",
    "sql-formatter": "^15.5.2",
    "stripe": "^18.1.1",
    "swr": "^2.3.3",
    "trino-client": "^0.2.6",
