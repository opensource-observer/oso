  "private": true,
  "scripts": {
    "build": "pnpm build:next",
    "build:next": "next build",
    "build:prod": "pnpm codegen && pnpm build:next",
    "codegen": "pnpm supabase:codegen && pnpm graphql:codegen && pnpm graphql:rover",
    "deploy": "pnpm build:prod",
    "dev": "pnpm codegen && next dev",
    "graphql:codegen": "dotenv -e .env.local -- graphql-codegen --require tsconfig-paths/register --config ./lib/graphql/codegen.ts && prettier --write ./lib/graphql/codegen.ts",
    "graphql:rover": "rover supergraph compose --elv2-license 'accept' --config ./supergraph-config.yaml > ./app/api/v1/graphql/supergraph.graphql",
    "lint": "tsc --noEmit && next lint && pnpm lint:eslint && pnpm lint:prettier",
    "lint:eslint": "eslint --ignore-path ../../.gitignore --ignore-path .gitignore --max-warnings 0 . --fix",
    "lint:prettier": "prettier --ignore-path ../../.gitignore --ignore-path .gitignore --log-level warn --check **/*.{js,jsx,ts,tsx,sol,md,json}",
    "start": "next start",
    "start-debug": "NODE_OPTIONS='--inspect' next dev",
    "supabase:codegen": "pnpm supabase:gentypes && pnpm supabase:zod",
    "supabase:gentypes": "npx supabase gen types typescript --project-id 'dfjsvzdlhmtnsqbfmcpm' --schema public > ./lib/types/supabase.ts && prettier --write ./lib/types/supabase.ts",
    "supabase:gentypes-local": "npx supabase gen types typescript --db-url 'postgresql://postgres:postgres@127.0.0.1:54322/postgres' --schema public > ./lib/types/supabase.ts && prettier --write ./lib/types/supabase.ts && pnpm supabase:zod",
    "supabase:zod": "supazod -i ./lib/types/supabase.ts -o ./lib/types/schema.ts -t ./lib/types/schema-types.ts && eslint --fix ./lib/types/schema.ts ./lib/types/schema-types.ts",
    "test": "dotenv -e .env.local -- jest --ci --passWithNoTests",
    "test:watch": "jest --watch",
    "update:deps": "ncu -i",
    "storybook": "storybook dev -p 3000",
    "build:storybook": "storybook build"
  },
